{
  "name": "svelte-todo",
  "version": "1.0.1",
  "description": "Sample application for Nimview",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marcomq/nimview.git"
  },
  "scripts": {
    "build": "rollup -c && inliner -m --videos --preserve-comments dist/index.html > dist/inlined.html",
    "devbuild": "rollup -c",
    "dev": "rollup -c -w",
    "dev-ie": "(set USE_BABEL=1 || export USE_BABEL=1) && rollup -c -w",
    "start": "sirv public --no-clear"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/runtime": "^7.15.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@tauri-apps/tauri-inliner": "^1.13.2",
    "rollup": "^2.56.3",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.14",
    "svelte": "^3.42.4"
  },
  "dependencies": {
    "@palsch/svelte-sortablejs": "^1.2.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.17.2",
    "jquery": "^3.6.0",
    "nimview": "^1.1.3"
  }
}
